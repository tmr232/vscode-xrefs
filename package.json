{
  "main": "./dist/extension.js",
  "module": "index.ts",
  "type": "module",
  "dependencies": {
    "@types/vscode": "^1.94.0",
    "web-tree-sitter": "0.25.4"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.5.0",
    "esbuild": "^0.25.0",
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/bun": "latest",
    "@types/emscripten": "^1.40.1",
    "@types/eslint__js": "^8.42.3",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.28.0",
    "oxlint": "1.3.0",
    "tree-sitter-python": "^0.23.6",
    "typescript-eslint": "^8.35.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.6.2"
  },
  "scripts": {
    "dev": "echo 'Open this directory in VSCode and then run your extension with F5 or `Run and Debug > Run Extension`!'",
    "build": "bun run ./scripts/build-with-esbuild.ts",
    "watch": "bun run ./scripts/watch-with-esbuild.ts",
    "vsce-package": "bun run vsce package",
    "vsce-publish": "bun run vsce publish",
    "package": "bun run build && bun run vsce-package",
    "publish": "bun run package && bun run vsce-publish",
    "oxlint-fix": "bunx oxlint --ignore-path=oxlint-ignore.txt --fix -D correctness -D perf -D suspicious -A no-await-in-loop",
    "oxlint-ci": "bunx oxlint --ignore-path=oxlint-ignore.txt  -D correctness -D perf -D suspicious -A no-await-in-loop",
    "lint": "bunx biome check --fix && bun oxlint-fix && bunx eslint --fix && bunx tsc --noEmit",
    "ci": "bunx biome ci && bun oxlint-ci && bunx eslint && bunx tsc --noEmit",
    "generate-parsers": "bun run ./scripts/generate-parsers.ts"
  },
  "//": "START EXTENSION ATTRIBUTES",
  "publisher": "tamir-bahar",
  "name": "xrefs",
  "version": "1.2.0",
  "description": "XRefs",
  "displayName": "XRefs",
  "categories": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/tmr232/vscode-xrefs.git"
  },
  "preview": true,
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "xrefs.findAllXrefs",
        "title": "Find Xrefs",
        "enablement": "editorHasReferenceProvider"
      },
      {
        "command": "xrefs.findWriteXrefs",
        "title": "Find Write-Xrefs",
        "enablement": "editorHasReferenceProvider && editorLangId == 'python'"
      },
      {
        "command": "xrefs.findReadXrefs",
        "title": "Find Read-Xrefs",
        "enablement": "editorHasReferenceProvider && editorLangId == 'python'"
      },
      {
        "command": "xrefs.findImportXrefs",
        "title": "Find Import-Xrefs",
        "enablement": "editorHasReferenceProvider && editorLangId == 'python'"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "xrefs.findAllXrefs",
          "when": "editorHasReferenceProvider",
          "group": "0_navigation@42"
        },
        {
          "command": "xrefs.findWriteXrefs",
          "when": "editorHasReferenceProvider && editorLangId == 'python'",
          "group": "0_navigation@43"
        },
        {
          "command": "xrefs.findReadXrefs",
          "when": "editorHasReferenceProvider && editorLangId == 'python'",
          "group": "0_navigation@44"
        }
      ]
    }
  },
  "engines": {
    "vscode": "^1.86.0"
  },
  "patchedDependencies": {
    "web-tree-sitter@0.25.3": "patches/web-tree-sitter@0.25.3.patch",
    "web-tree-sitter@0.25.4": "patches/web-tree-sitter@0.25.4.patch"
  }
}